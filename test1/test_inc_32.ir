function _print_int8(str:*char, n:int8) {
	asm_set_reg "%eax", str;
	asm_set_reg "%bl", n;
	asm "movsbl %bl, %ebx\n";

	asm "pushl %eax\n";
	asm "calll printf\n";
	asm "addl $4, %esp\n";

	asm_set_reg "%eax", "%d\n";
	asm "pushl %ebx\n";
	asm "pushl %eax\n";
	asm "calll printf\n";
	asm "addl $8, %esp\n";
}


